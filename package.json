{
  "name": "tddtest",
  "version": "0.0.1",
  "description": "",
  "repository": "https://github.com/jozsefs/tdd-minimal-boilerplate",
  "main": "index.js",
  "scripts": {
    "bundle": "node_modules\\.bin\\browserify -o bundle.js all_test_file.js",
    "test": "node_modules\\.bin\\mocha -u tdd -R spec tests/*.js --compilers js:babel-core/register",
    "coverage": "node_modules\\.bin\\istanbul cover node_modules/mocha/bin/_mocha -- -u tdd -R spec tests/*.js --compilers js:babel-core/register",
    "jshint": "node_modules\\.bin\\jshint libs tests",
    "jscs": "node_modules\\.bin\\jscs -c .jscsrc libs tests",
    "jsdoc": "node_modules\\.bin\\jsdoc -c .jsdocrc libs",
    "write": "node load_all_test_files.js",
    "all": "npm run jshint && npm run jscs && npm run jsdoc && npm run coverage && npm run write && npm run bundle"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "async": "~0.9.0",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "~5.11.0",
    "chai": "3.2.0",
    "istanbul": "~0.3.2",
    "jscs": "~1.6.0",
    "jsdoc": "^3.3.0-alpha9",
    "jshint": "~2.5.5",
    "mocha": "2.2.5",
    "sinon": "~1.10.3",
    "underscore": "~1.7.0"
  }
}
